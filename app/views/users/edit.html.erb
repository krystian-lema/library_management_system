<!-- app/views/users/edit.html.erb -->

<h1>Edit</h1>

<%= form_for @user do |f| %>

	<p>
		<%= f.label :username %> <br>
		<%= f.text_field(:username) %>
	</p>

	<p>
		<%= f.label :email %> <br>
		<%= f.text_field(:email) %>
	</p>

  <%= f.submit "Submit" %> <br>
  <%= link_to 'Cancel changes', root_path %>

<% end %>